<import src="../../../common/head.axml"></import>
<import src="../../../common/foot.axml"></import>

<view  class="container">
  <template  is="head" data="{{title: 'vibrate/Long/Short'}}"></template>

  <view  class="page-body">
    <view  class="page-section">
      <view  class="btn-area">
        <wx-button  type="primary" onTap="vibrateLong">长振动</wx-button>
        <wx-button  type="default" onTap="vibrateShort">短振动</wx-button>
      </view>
    </view>
  </view>

  <template  is="foot"></template>
</view>
